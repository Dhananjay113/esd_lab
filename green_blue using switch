#include"msp.h"
/**
* main.c
*/
voiddelay(unsignedlong integer){
volatileunsignedlongi;
for(i=0;i<integer;i++){
}
}
voidmain(void)
{
WDT_A->CTL = WDT_A_CTL_PW | WDT_A_CTL_HOLD; // stop
watchdog timer
P2->DIR |=BIT1;
P2->DIR |=BIT2;
P2->DIR |=BIT0;

P1->DIR&= ~BIT1;
P1->REN |= BIT1; // Enable pull-up resistor
P1->OUT |= BIT1;
while(1)
{
P2->OUT|= BIT2;
delay(100000);
P2->OUT&= ~BIT2;
delay(100000);
P2->OUT|= BIT1;
delay(100000);
P2->OUT&= ~BIT1;
delay(100000);

}
}
